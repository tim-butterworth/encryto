{
    "compilerOptions": {
        "module": "commonjs",
        "lib": [
            "es5",
            "es6",
            "dom"
        ],
        "rootDir": "src",
        "forceConsistentCasingInFileNames": true,
        "noImplicitAny": true,
        "strict": true,
        "noImplicitReturns": true,
        "noUnusedLocals": true,
        "noUncheckedIndexedAccess": false,
    },
    "exclude": [
        "./node_modules"
    ]
}